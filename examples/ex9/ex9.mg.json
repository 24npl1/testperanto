{
    "distributions": [
        {"name": "verb", "type": "pyor", "strength": 150, "discount": 0.35},
        {"name": "noun", "type": "pyor", "strength": 150, "discount": 0.35},
        {"name": "noun~$y1", "type": "pyor", "strength": 5, "discount": 0.2},
        {"name": "adj", "type": "pyor", "strength": 50, "discount": 0.45},
        {"name": "adj~$y1", "type": "pyor", "strength": 0.1, "discount": 0.85},
        {"name": "qnc", "type": "pyor", "strength": 10, "discount": 0.2},
        {"name": "qnc~subj", "type": "pyor", "strength": 10, "discount": 0.2},
        {"name": "qnc~obj", "type": "pyor", "strength": 10, "discount": 0.2},
        {"name": "qnc~iobj", "type": "pyor", "strength": 10, "discount": 0.2},
        {"name": "qnc~subj~$z1", "type": "pyor", "strength": 5, "discount": 0.2},
        {"name": "qnc~obj~$z1", "type": "pyor", "strength": 5, "discount": 0.2},
        {"name": "qnc~iobj~$z1", "type": "pyor", "strength": 5, "discount": 0.2}
    ],
    "macros": [
        {"rule": "$qstart -> $qdecl"},
        {
            "rule": "$qdecl -> (S v~$z1 $qvprop $qnc~$z2 $qnc~$z3 $qnc~$z4)",
            "zdists": ["verb", "qnc~subj~$z1", "qnc~obj~$z1", "qnc~iobj~$z1"]
        },
        {"rule": "$qvprop -> (VPROP $qtense)"},
        {"rule": "$qtense -> (TENSE present)"},
        {"rule": "$qtense -> (TENSE perfect)"},
        {"rule": "$qnc~$y1 -> (NC nc~$z1 $qncprop $qncmod~$z1)", "zdists": ["noun~$y1"]},
        {"rule": "$qncprop -> (NCPROP $qdef $qcount)"},
        {"rule": "$qdef -> (DEF def)"},
        {"rule": "$qdef -> (DEF indef)"},
        {"rule": "$qcount -> (COUNT sng)"},
        {"rule": "$qcount -> (COUNT plu)"},
        {"rule": "$qncmod~$y1 -> (ADJC adjc~$z1)", "zdists": ["adj~$y1"]}
    ]
}

