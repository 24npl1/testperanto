{
    "distributions": [
        {"name": "count", "type": "uniform", "domain": ["sng", "plu"]}
    ],
    "rules": [
        {"rule": "($qstart $x1) -> ($qevent $x1)"},
        {"rule": "($qevent (EVENT (inst $x1) (args (arg0 $x2) $x3) $x4)) -> (S ($qarg0.$z1 (arg0 $x2)) ($qvb.$z1 $x1) ($qarg1 $x3))", "zdists": ["count"]},
        {"rule": "($qccomp (EVENT (inst $x1) (args (arg1 $x2)) (mods $x3))) -> (S ($qvbinf $x1) ($qarg1 (arg1 $x2)))"},
        {"rule": "($qentity.$y1 (ENTITY (inst $x1) $x2)) -> ($qnn.$y1 $x1)"},
        {"rule": "($qarg0.$y1 (arg0 $x1)) -> ($qentity.$y1 $x1)"},
        {"rule": "($qarg1 (arg1 (ENTITY $x1 $x2))) -> ($qentity.$z1 (ENTITY $x1 $x2))", "zdists": ["count"]},
        {"rule": "($qarg1 (arg1 (EVENT $x1 $x2 $x3))) -> ($qccomp (EVENT $x1 $x2 $x3))"},
        {"rule": "($qvbinf vb.$y1) -> (VP (TO to) (VB (@vb (STEM vb.$y1) (PERSON 1) (COUNT sng) (DEF def) (TENSE present))))"},
        {"rule": "($qvb.$y1 vb.$y2) -> (VB (@vb (STEM vb.$y2) (PERSON 3) (COUNT $y1) (DEF def) $qtense))"},
        {"rule": "($qnn.$y1 nn.$y2) -> (NP $qdt.$z1 (NN (@nn (STEM nn.$y2) (PERSON 3) (COUNT $y1) (DEF def))))"},
        {"rule": "$qdt.$y1 -> (DT the)"},
        {"rule": "$qdt.sng -> (DT a)"},
        {"rule": "$qdt.plu -> (DT -null-)"},
        {"rule": "$qtense -> (TENSE perfect)"},
        {"rule": "$qtense -> (TENSE present)"}
    ]
}
